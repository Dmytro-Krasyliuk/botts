@bot.message_handler(commands=['start'])
def send_text(message):
    if message.text:
        bot.send_message(message.chat.id, '–û—Ç–ø—Ä–∞–≤—å—Ç–µ —Å–≤–æ–π e-mail —Å—é–¥–∞ @TelegramStarinets')